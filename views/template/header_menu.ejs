<div class="main__logo">
  <h1 class="main__logo--title">
    <a class="main__logo--link" href="/">
      <img class="main__logo--img" src="/img/logo/nav-log.png" alt="logo-img">
    </a>
  </h1>
</div>
<div class="header__menu d-none d-lg-block">
  <nav class="header__menu--navigation">
    <ul class="d-flex">                
      <li class="header__menu--items"> <a class="header__menu--link" href="<%- session.home %>">Home </a> </li>
      <% if(session.phone){ %>
      <li class="header__menu--items"> <a class="header__menu--link" href="/saran">Saran </a> </li>
      <% } %>
      <li class="header__menu--items"> <a class="header__menu--link" href="/about">Tentang Kami </a> </li>
    </ul>
  </nav>
</div>
<div class="header__account">
  <ul class="d-flex">
    <li class="header__account--items  header__account--search__items">
      <a class="header__account--btn search__open--btn" href="javascript:void(0)" data-offcanvas>
        <svg class="product__items--action__btn--svg" xmlns="http://www.w3.org/2000/svg" width="22.51" height="20.443" viewBox="0 0 512 512"><path d="M221.09 64a157.09 157.09 0 10157.09 157.09A157.1 157.1 0 00221.09 64z" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="32"/><path fill="none" stroke="currentColor" stroke-linecap="round" stroke-miterlimit="10" stroke-width="32" d="M338.29 338.29L448 448"/></svg>
        <span class="visually-hidden">Search</span>  
      </a>
    </li>
    <% if(session.phone && (session.userType == 1 || session.userType == 4 || session.userType == 5 || session.userType == 7)){%> 
      <li class="header__account--items d-lg-block">
        <% if(session.userType == 1){%> 
          <a class="header__account--btn" href="/admin/client">
        <%}else if(session.userType == 4){%>  
          <a class="header__account--btn" href="/admin/jadwal">
        <%}else if(session.userType == 5 || session.userType == 7){%>
          <a class="header__account--btn" href="/user/profile">
        <%}%>
          <svg xmlns="http://www.w3.org/2000/svg" width="20.51" height="19.443" viewBox="0 0 512 512">
            <path d="M344 144c-3.92 52.87-44 96-88 96s-84.15-43.12-88-96c-4-55 35-96 88-96s92 42 88 96z" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="32" />
            <path d="M256 304c-87 0-175.3 48-191.64 138.6C62.39 453.52 68.57 464 80 464h352c11.44 0 17.62-10.48 15.65-21.4C431.3 352 343 304 256 304z" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="32" />
          </svg>
          <span class="visually-hidden">My account</span>
        </a>
      </li>
    <%}%>
    <%if(session.userType == 5 || session.userType == 7){%>  
      <li class="header__account--items">
        <a class="header__account--btn minicart__open--btn openCard" href="javascript:void(0)" data-offcanvas>
          <svg xmlns="http://www.w3.org/2000/svg" width="16.706" height="15.534" viewBox="0 0 14.706 13.534">
            <g transform="translate(0 0)">
              <g>
                <path data-name="Path 16787" d="M4.738,472.271h7.814a.434.434,0,0,0,.414-.328l1.723-6.316a.466.466,0,0,0-.071-.4.424.424,0,0,0-.344-.179H3.745L3.437,463.6a.435.435,0,0,0-.421-.353H.431a.451.451,0,0,0,0,.9h2.24c.054.257,1.474,6.946,1.555,7.33a1.36,1.36,0,0,0-.779,1.242,1.326,1.326,0,0,0,1.293,1.354h7.812a.452.452,0,0,0,0-.9H4.74a.451.451,0,0,1,0-.9Zm8.966-6.317-1.477,5.414H5.085l-1.149-5.414Z" transform="translate(0 -463.248)" fill="#fefefe" />
                <path data-name="Path 16788" d="M5.5,478.8a1.294,1.294,0,1,0,1.293-1.353A1.325,1.325,0,0,0,5.5,478.8Zm1.293-.451a.452.452,0,1,1-.431.451A.442.442,0,0,1,6.793,478.352Z" transform="translate(-1.191 -466.622)" fill="#fefefe" />
                <path data-name="Path 16789" d="M13.273,478.8a1.294,1.294,0,1,0,1.293-1.353A1.325,1.325,0,0,0,13.273,478.8Zm1.293-.451a.452.452,0,1,1-.431.451A.442.442,0,0,1,14.566,478.352Z" transform="translate(-2.875 -466.622)" fill="#fefefe" />
              </g>
            </g>
          </svg>
          <span class="items__count">0</span>
        </a>
      </li>
    <% } %> 
    <% if(!session.phone){ %>
      <li class="header__account--items d-none d-lg-block">
        <a class="header__account--btn" href="/login">
          <svg xmlns="http://www.w3.org/2000/svg" width="20.51" height="19.443" viewBox="0 0 512 512">
            <path d="M344 144c-3.92 52.87-44 96-88 96s-84.15-43.12-88-96c-4-55 35-96 88-96s92 42 88 96z" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="32" />
            <path d="M256 304c-87 0-175.3 48-191.64 138.6C62.39 453.52 68.57 464 80 464h352c11.44 0 17.62-10.48 15.65-21.4C431.3 352 343 304 256 304z" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="32" />
          </svg>
          <span class="visually-hidden">My account</span>
        </a>
      </li>
    <%}%>  

    <% if(session.phone){%> 
      <li class="header__account">
        <a class="header__account--btn" href="/logout">
          <svg fill="currentColor" width="18.51" height="17.443" viewBox="0 0 20 20">
            <path d="M3.24,7.51c-0.146,0.142-0.146,0.381,0,0.523l5.199,5.193c0.234,0.238,0.633,0.064,0.633-0.262v-2.634c0.105-0.007,0.212-0.011,0.321-0.011c2.373,0,4.302,1.91,4.302,4.258c0,0.957-0.33,1.809-1.008,2.602c-0.259,0.307,0.084,0.762,0.451,0.572c2.336-1.195,3.73-3.408,3.73-5.924c0-3.741-3.103-6.783-6.916-6.783c-0.307,0-0.615,0.028-0.881,0.063V2.575c0-0.327-0.398-0.5-0.633-0.261L3.24,7.51 M4.027,7.771l4.301-4.3v2.073c0,0.232,0.21,0.409,0.441,0.366c0.298-0.056,0.746-0.123,1.184-0.123c3.402,0,6.172,2.709,6.172,6.041c0,1.695-0.718,3.24-1.979,4.352c0.193-0.51,0.293-1.045,0.293-1.602c0-2.76-2.266-5-5.046-5c-0.256,0-0.528,0.018-0.747,0.05C8.465,9.653,8.328,9.81,8.328,9.995v2.074L4.027,7.771z"></path>
          </svg>
          <span class="visually-hidden">Logout</span>
        </a>
      </li>
    <%}%>
  </ul>
</div>

<div class="predictive__search--box ">
  <div class="predictive__search--box__inner">
      <h2 class="predictive__search--title">Pencarian Menu</h2>
      <form class="predictive__search--form" action="#">
          <label class="searchInput">
              <input class="predictive__search--input inputsearch" placeholder="Cari makanan kesukaan anda" type="text">
              <div class="resultBox">
                <li>Ayam Rica Rica - Rp 12.000</li>
                <li>Cumi Asam Manis - Rp 12.000</li>
                <li>Dendeng Balado - Rp 12.000</li>
              </div>
          </label>
          <button class="predictive__search--button" aria-label="search button"><svg class="product__items--action__btn--svg" xmlns="http://www.w3.org/2000/svg" width="30.51" height="25.443" viewBox="0 0 512 512"><path d="M221.09 64a157.09 157.09 0 10157.09 157.09A157.1 157.1 0 00221.09 64z" fill="none" stroke="currentColor" stroke-miterlimit="10" stroke-width="32"/><path fill="none" stroke="currentColor" stroke-linecap="round" stroke-miterlimit="10" stroke-width="32" d="M338.29 338.29L448 448"/></svg>  </button>
      </form>
  </div>
  <button class="predictive__search--close__btn" aria-label="search close" data-offcanvas>
      <svg class="predictive__search--close__icon" xmlns="http://www.w3.org/2000/svg" width="40.51" height="30.443"  viewBox="0 0 512 512"><path fill="currentColor" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="32" d="M368 368L144 144M368 144L144 368"/></svg>
  </button>
</div>