            <p class="product__details--info__desc" id="searchdescription"></p>
          </div>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <% if(session.userType == '7'){ %>
          <div id="searchbtn"></div>
        <% } %>
      </div>
    </div>
