    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Menu Premium <span class="searchname"></span></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <div class="product__details--media mb-0">
          <img class="product__media--preview__items--img" src="" alt="product-media-img" id="searchimage">
          <div class="product__details--info">
            <h3 class="product__details--info__title mt-3 searchname"></h3>
            <div class="product__details--info__price mb-15">
              <span class="current__price" id="searchrate"></span>
