<!doctype html>
<html lang="en">
	<%- include("../template/css.ejs") %>
	<body>
		<header class="header__section">
			<%- include("../template/login_info.ejs") %>
			<div class="main__header main__header2 header__sticky sticky" style="margin-top: 36px;">
				<div class="container">
					<div class="main__header--inner position__relative d-flex justify-content-between align-items-center">
						<div class="offcanvas__header--menu__open ">
							<a class="offcanvas__header--menu__open--btn" href="javascript:void(0)" data-offcanvas>
							</a>
						</div>
						<%- include("../template/header_menu.ejs") %>
					</div>
				</div>
			</div>
			<%- include("../template/toolbar.ejs") %>
			<%- include("../template/search_box.ejs") %>
		</header>
		<!-- End header area -->
		<main class="main__content_wrapper">
			<!-- my account section start -->
			<section class="section--padding mt-100">
				<div class="container">
					<div class="row">
                    <div class="col-lg-12 mt-3">
							<nav>
								<ol class="cd-breadcrumb custom-separator">
									<li><a href="javascript:void(0)">Admin</a></li>
									<li class="current"><em>Jadwal Detail</em></li>
								</ol>
							</nav>
						</div>
						<div class="col-lg-3 mb-3">
							<div class="shop__sidebar--widget widget__area d-lg-block">
								<div class="single__widget widget__bg">
									<h2 class="widget__title h3">Menu</h2>
									<ul class="widget__categories--menu">
                    					<% if(session.userType == '1'){ %>
										<li class="widget__categories--menu__list">
											<a href="/admin/client" class="widget__categories--menu__label d-flex align-items-center p-menu">
											<i class="fas fa-star"></i> 
											<span class="widget__categories--menu__text">Daftar Client</span>
											</a>
										</li>
										<li class="widget__categories--menu__list">
											<a href="/admin/paket" class="widget__categories--menu__label d-flex align-items-center p-menu">
											<i class="fas fa-briefcase"></i> 
											<span class="widget__categories--menu__text">Daftar Paket</span>
											</a>
										</li>
                    					<% } %>
										<li class="widget__categories--menu__list">
											<a href="/admin/jadwal" class="widget__categories--menu__label d-flex align-items-center p-menu">
											<i class="fas fa-list text-warning"></i> 
											<span class="widget__categories--menu__text text-warning">Daftar Jadwal</span>
											</a>
										</li>
										<li class="widget__categories--menu__list">
											<a href="/admin/user" class="widget__categories--menu__label d-flex align-items-center p-menu">
											<i class="fas fa-user"></i> 
											<span class="widget__categories--menu__text">Daftar User</span>
											</a>
										</li>
									</ul>
								</div>
							</div>
						</div>
					<div class="col-lg-9">
              <div>
                <!-- <h2 class="account__content--title h3 mb-20">Orders History</h2> -->
                <div>
					<div class="container">
						<div class="shipping__section--inner d-flex">
							<div class="shipping__items">
								<div class="shipping__items--icon">
									<svg xmlns="http://www.w3.org/2000/svg" width="32.955" height="23.967" viewBox="0 0 32.955 23.967">
										<path d="M29.461,9.992H24.967V4H4A3,3,0,0,0,1,7V23.473H4a4.494,4.494,0,0,0,8.988,0h8.988a4.494,4.494,0,1,0,8.988,0h3v-7.49ZM8.49,25.72a2.247,2.247,0,1,1,2.247-2.247A2.244,2.244,0,0,1,8.49,25.72ZM28.712,12.239l2.936,3.745H24.967V12.239ZM26.465,25.72a2.247,2.247,0,1,1,2.247-2.247A2.244,2.244,0,0,1,26.465,25.72Z" transform="translate(-1 -4)" fill="currentColor" opacity="0.73"></path>
									</svg>
								</div>
								<div class="shipping__items--content text-center">
									<h2 class="shipping__items--title h3">Total QTY</h2>
									<p class="shipping__items--desc"><%= jadwal.qty%></p>
								</div>
							</div>
							<div class="shipping__items">
								<div class="shipping__items--icon">
									<svg xmlns="http://www.w3.org/2000/svg" width="27.63" height="38.407" viewBox="0 0 27.63 38.407">
										<path d="M17.615,6.106V1L10.808,7.808l6.808,6.808V9.509A10.219,10.219,0,0,1,27.826,19.721a9.991,9.991,0,0,1-1.191,4.765l2.485,2.485a13.591,13.591,0,0,0-11.5-20.865Zm0,23.826A10.219,10.219,0,0,1,7.4,19.721,9.991,9.991,0,0,1,8.6,14.955L6.11,12.471a13.591,13.591,0,0,0,11.5,20.865v5.106l6.808-6.808-6.808-6.808Z" transform="translate(-3.8 -0.517)" fill="currentColor" stroke-width="0.4" opacity="0.7"></path>
									</svg>  
								</div>
								<div class="shipping__items--content text-center">
									<h2 class="shipping__items--title h3">Total QTY Akhir</h2>
									<p class="shipping__items--desc"><%= jadwal.qty_perubahan%></p>
								</div>
							</div>
							<div class="shipping__items">
								<div class="shipping__items--icon">
									<svg xmlns="http://www.w3.org/2000/svg" width="25" height="22.143" viewBox="0 0 25 22.143">
										<path d="M81.251,82.1c2.151,0,3.12-.959,3.12-2.149S83.4,77.8,81.251,77.8a6.99,6.99,0,0,0-4.694,1.8,3.4,3.4,0,0,0-1.609-.414,3.306,3.306,0,0,0-1.54.385A7,7,0,0,0,68.746,77.8c-2.157,0-3.12.964-3.12,2.149s.96,2.149,3.12,2.149h5.189V99.943H76.06V82.1Zm0-2.862c.709,0,1.554.126,1.554.713s-.846.721-1.554.721H77.7l-.029-.066A5.35,5.35,0,0,1,81.251,79.236ZM72.206,80.67H68.749c-.709,0-1.566-.128-1.566-.721s.854-.713,1.566-.713a5.33,5.33,0,0,1,3.526,1.321Zm-9.706,3.5v2.87H73.44v-4.3H64.066A1.505,1.505,0,0,0,62.5,84.174Zm1.566,14.333a1.5,1.5,0,0,0,1.563,1.434H73.44V88.472H64.066ZM85.934,82.74H76.628v4.306H87.5V84.174A1.507,1.507,0,0,0,85.934,82.74Zm-9.306,17.2h7.743a1.507,1.507,0,0,0,1.566-1.434V88.472H76.628Z" transform="translate(-62.5 -77.8)" fill="currentColor" opacity="0.7"></path>
									</svg>
								</div>
								<div class="shipping__items--content text-center">
									<h2 class="shipping__items--title h3">Custom Order</h2>
									<p class="shipping__items--desc"><%= order.banyak%></p>
								</div>
							</div>
							<div class="shipping__items">
								<div class="shipping__items--icon">
									<svg xmlns="http://www.w3.org/2000/svg" width="27.787" height="22.735" viewBox="0 0 27.787 22.735">
										<path d="M26.261,3H3.526A2.534,2.534,0,0,0,1,5.526V23.209a2.534,2.534,0,0,0,2.526,2.526H26.261a2.534,2.534,0,0,0,2.526-2.526V5.526A2.534,2.534,0,0,0,26.261,3ZM14.894,13.1H3.526V10.578H14.894Zm0-5.052H3.526V5.526H14.894Z" transform="translate(-1 -3)" fill="currentColor" opacity="0.7"></path>
									</svg>
								</div>
								<div class="shipping__items--content text-center">
									<h2 class="shipping__items--title h3">IDR Custom</h2>
									<p class="shipping__items--desc">Rp <%= order.total%></p>
								</div>
							</div>
						</div>
					</div>

					<table class="cart__table--inner">
						<tbody class="cart__table--body">
							<tr class="cart__table--body__items">
								<td class="cart__table--body__list">
									Nama Perusahaan
								</td>
								<td class="cart__table--body__list text-right">
									<%= jadwal.client%>
								</td>
							</tr>
							<tr class="cart__table--body__items">
								<td class="cart__table--body__list">
									Tanggal Pemesanan
								</td>
								<td class="cart__table--body__list text-right">
									<%= jadwal.dates%>
								</td>
							</tr>
							<tr class="cart__table--body__items">
								<td class="cart__table--body__list">
									Waktu Pemesanan
								</td>
								<td class="cart__table--body__list text-right">
									<%= jadwal.waktu%>
								</td>
							</tr>
							<tr class="cart__table--body__items">
								<td class="cart__table--body__list">
									Paket
								</td>
								<td class="cart__table--body__list text-right">
									<%= jadwal.paket%>
								</td>
							</tr>
							<tr class="cart__table--body__items">
								<td class="cart__table--body__list">
									Menu Sehat
								</td>
								<td class="cart__table--body__list text-right">
									<%= jadwal.sehat%>
								</td>
							</tr>
							<tr class="cart__table--body__items">
								<td class="cart__table--body__list">
									Status
								</td>
								<td class="cart__table--body__list text-right">
									<%= jadwal.statusAlias%>
								</td>
							</tr>
							<tr class="cart__table--body__items">
								<td class="cart__table--body__list">
									Tagihan Paket
								</td>
								<td class="cart__table--body__list text-right">
									Rp <%- parseInt(jadwal.total)%>
								</td>
							</tr>
							<tr class="cart__table--body__items">
								<td class="cart__table--body__list">
									Total Tagihan
								</td>
								<td class="cart__table--body__list text-right">
									Rp <%- parseInt(jadwal.total) + parseInt(order.total)%>
								</td>
							</tr>
						</tbody>
					</table>
					<br>
					<table id="jadwal" class="display table table-striped">
						<thead>
							<tr>
								<th>No.</th>
								<th>Nama</th>
								<th>Menu</th>
								<th>Total</th>
							</tr>
						</thead>
						<tbody>

						</tbody>
					</table>
                </div>
              </div>
            </div>
			</div>
		</div>
	</section>
		<!-- my account section end -->
	</main>
	<%- include("../template/footer.ejs") %>
	<%- include("../template/js.ejs") %>
    <script src="https://cdn.jsdelivr.net/npm/jquery-table2excel@1.1.1/dist/jquery.table2excel.min.js"></script>
    <script type="text/javascript">
      $(document).ready(function(){
		$.ajax({
            url: "/admin/jadwal/detail/get/<%- kode %>",
            type: "GET",
            dataType: "json",
            success: function(ress) {
              	if(ress.response == 'Successful'){
					var html = ""
					var no = 1;
					$.each(ress.result, function(index, val){
						var html2 = "<ol>"
						$.each(ress.resultlist, function(index2, val2){
							if(val.id == val2.order_id){
								html2 += `<li style="list-style: auto;">${val2.name} (QTY : ${val2.qty}) - Rp ${val2.rate}</li>`
							}
						})
						html2+= "</ol>"
						html += `<tr><td>${no++}</td><td>${val.username ? val.username : 'Nama Tidak Ditemukan'}</td><td>${html2}</td><td>Rp ${val.total}</td></tr>`;
						html2 = '';
					})
					$("#jadwal").find('tbody').html(html)
					$("#jadwal").dataTable( {
						dom: 'Bfrtip',
						buttons: [
							'copy', 'csv', 'excel', 'pdf', 'print'
						]
					} )
				}
            }
		})
      })
    </script>
	</body>
</html>